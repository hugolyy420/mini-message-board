<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <ul>
      <% messages.forEach((message) => {%>
      <li>
        <p><%= message.user %>: <%= message.text%></p>
        <p>Added at: <%= message.added%></p>
      </li>
      <% }) %>
    </ul>
    <a href="/new">Add new message</a>
  </body>
</html>
